{
  "services": [
    {
      "name": "homeassistant",
      "image": "ghcr.io/home-assistant/home-assistant:stable",
      "isMain": true,
      "internalPort": 8123,
      "volumes": [
        {
          "source": "./data",
          "target": "/config"
        }
      ],
      "env": {
        "TZ": "Europe/Paris"
      }
    }
  ]
}